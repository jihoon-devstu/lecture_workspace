<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "mybatis-3-mapper.dtd">
<mapper namespace="SubCategory">

	<select id="selectByTopCategoryId" parameterType="int" resultType="SubCategory">
		select *from subcategory where topcategory_id=#{topcategory_id}
	</select>
	
	<select id="select" resultType="SubCategory" parameterType="int">
		select *from subcategory where subcategory_id=#{subcategory_id}
	</select>

</mapper>