<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "mybatis-3-mapper.dtd">
<mapper namespace="Color">

	<select id="selectAll" resultType="Color">
		select *from color
	</select>

	<select id="select" parameterType="int" resultType="Color">
		select *from color where color_id=#{color_id}
	</select>
</mapper>