<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "mybatis-3-mapper.dtd">
<mapper namespace="TopCategory">

	<select id="selectAll" resultType="TopCategory">
		select *from topcategory
	</select>
	
	<select id="select" resultType="TopCategory" parameterType="int">
		select topcategory_id , top_name from topcategory where topcategory_id = #{topcategory_id}
	</select>
	
</mapper>